{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "northeurope"
        },
        "containerName": {
            "value": "<YOUR-CONTAINER-INSTANCE-NAME>"
        },
        "imageType": {
            "value": "Private"
        },
        "imageName": {
            "value": "<YOUR-CONTAINER-INSTANCE-NAME>.azurecr.io/<YOUR-IMAGE-NAME>:<IMAGE-TAG>"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1"
        },
        "restartPolicy": {
            "value": "Never"
        },
        "imageRegistryLoginServer": {
            "value": "<YOUR-CONTAINER-INSTANCE-NAME>.azurecr.io"
        },
        "imageUsername": {
            "value": null
        },
        "imagePassword": {
            "value": null
        },
        "ipAddressType": {
            "value": "Private"
        },
        "ports": {
            "value": [
                {
                    "port": "80",
                    "protocol": "TCP"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "<YOUR-CONTAINER-INSTANCE-DNS-LABEL>"
        }
    }
}